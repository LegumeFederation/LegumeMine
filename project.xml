<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="source.location" location="../bio/sources/"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="legumemine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>

  <sources>

    <!-- chado genomics - bean, soybean, barrel medic -->
    <source name="chado-genomics1" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847 3880 3885 3398"/>
      <property name="dataSetTitle" value="LIS chado genomic data"/>
      <property name="dataSourceName" value="LIS chado genomic data"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.SequenceProcessor"/>
    </source>

    <!-- chado genomics - peanut -->
    <source name="chado-genomics2" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="peanutbase"/>
      <property name="organisms" value="3398 3817 3818 130453 130454"/>
      <property name="dataSetTitle" value="Peanutbase chado data"/>
      <property name="dataSourceName" value="Peanutbase chado database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.SequenceProcessor"/>
    </source>

    <!-- chado genetics - bean, soybean, barrel medic -->
    <source name="chado-genetics1" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847 3880 3885 3398"/>
      <property name="dataSetTitle" value="LIS chado genetic data"/>
      <property name="dataSourceName" value="LIS chado genetic data"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GeneticProcessor"/>
    </source>

    <!-- chado genetics - peanut -->
    <source name="chado-genetics2" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="peanutbase"/>
      <property name="organisms" value="3398 3817 3818 130453 130454"/>
      <property name="dataSetTitle" value="Peanutbase chado data"/>
      <property name="dataSourceName" value="LIS Peanutbase database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GeneticProcessor"/>
    </source>

    <!-- chado featureprop - bean, soybean, barrel medic -->
    <source name="chado-featureprop1" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847 3880 3885 3398"/>
      <property name="dataSetTitle" value="LIS chado data"/>
      <property name="dataSourceName" value="LIS chado database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.FeaturePropProcessor"/>
    </source>

    <!-- chado featureprop - peanut -->
    <source name="chado-featureprop2" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="peanutbase"/>
      <property name="organisms" value="3398 3817 3818 130453 130454"/>
      <property name="dataSetTitle" value="Peanutbase chado data"/>
      <property name="dataSourceName" value="Peanutbase chado database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.FeaturePropProcessor"/>
    </source>

    <!-- chado homology attributes -->
    <source name="chado-homology" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847 3880 3885 130453 130454"/>
      <property name="homologue.organisms" value="3702 3847 3880 3885 130453 130454"/>
      <property name="phytozome.version" value="phytozome_10_2"/>
      <property name="dataSetTitle" value="LIS core chado"/>
      <property name="dataSourceName" value="LIS core chado database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.HomologyProcessor"/>
    </source>

    <!-- chado go - bean, soybean, barrel medic -->
    <source name="chado-go1" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847 3880 3885 3398"/>
      <property name="dataSetTitle" value="LIS chado data"/>
      <property name="dataSourceName" value="LIS chado database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GOAnnotationProcessor"/>
    </source>

    <!-- chado go - peanut -->
    <source name="chado-go2" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="peanutbase"/>
      <property name="organisms" value="3398 3817 3818 130453 130454"/>
      <property name="dataSetTitle" value="Peanutbase chado data"/>
      <property name="dataSourceName" value="Peanutbase chado database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GOAnnotationProcessor"/>
    </source>

    <!-- chado + reactome file - bean, soybean, barrel medic -->
    <source name="chado-reactome-file1" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="reactome.filename" location="/home/intermine/data/plant-reactome/reactome.txt"/>
      <property name="organisms" value="3885 3847 3880"/>
      <property name="dataSetTitle" value="Plant Reactome data"/>
      <property name="dataSourceName" value="LIS Tripal database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.ReactomeProcessor"/>
    </source>

    <!-- chado + reactome file - Arachis duranensis and ipaensis -->
    <source name="chado-reactome-file2" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="peanutbase"/>
      <property name="reactome.filename" location="/home/intermine/data/plant-reactome/reactome.txt"/>
      <property name="organisms" value="130453 130454"/>
      <property name="dataSetTitle" value="Plant Reactome data"/>
      <property name="dataSourceName" value="LIS peanutbase chado database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.ReactomeProcessor"/>
    </source>

    
    <!-- genetic markers from GFF files -->
    <source name="gff-geneticmarkers" type="legfed-geneticmarker-gff" dump="true">
      <property name="src.data.dir" location="/home/intermine/data/legumes-markers"/>
    </source>

    <!-- linkage group-QTL-marker relationships from tab-delimited CMap export files -->
    <source name="file-cmap" type="legfed-cmap-file" dump="true">
      <property name="src.data.dir" location="/home/intermine/data/legumes-cmap"/>
    </source>

    <!-- QTL-marker relationships from MySQL dump tab-delimited files -->
    <source name="file-qtlmarkers" type="legfed-qtlmarker-file" dump="true">
      <property name="src.data.dir" location="/home/intermine/data/legumes-qtl-markers"/>
    </source>

    <!-- synteny from GFF files -->
    <source name="gff-synteny" type="legfed-synteny-gff" dump="true">
      <property name="src.data.dir" location="/home/intermine/data/legumes-synteny"/>
    </source>

    <!-- gene ontology -->
    <source name="go" type="go">
        <property name="src.data.file" location="/home/intermine/data/go-annotation/go.obo"/>
    </source>

    <!-- sequence ontology -->
    <source name="so" type="so">
      <property name="src.data.file" location="../../bio/sources/so/so.obo"/>
    </source>
      
    <!-- update organism records -->
    <source name="entrez-organism" type="entrez-organism">
      <property name="src.data.file" location="build/organisms.xml"/>
    </source>

  </sources>

  <post-processing>

    <!-- legfed -->
    <post-process name="create-polypeptide-gene-references" dump="true"/>
    <post-process name="populate-gene-spanning-qtls" dump="true"/>

    <!-- order from flymine -->
    <post-process name="populate-located-features"/>
    <post-process name="create-references" />
    <post-process name="create-chromosome-locations-and-lengths"/>
    <post-process name="create-intergenic-region-features"/>
    <post-process name="transfer-sequences"/>
    <post-process name="populate-child-features"/>
    <post-process name="do-sources" dump="true"/>
    <post-process name="create-location-range-index"/>
    <post-process name="create-location-overlap-index"/>
    <post-process name="create-overlap-view" />
    <post-process name="create-attribute-indexes"/>
    <post-process name="summarise-objectstore"/>
    <post-process name="create-autocomplete-index"/>
    <post-process name="create-search-index"/>

  </post-processing>

</project>
