<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="source.location" location="../bio/sources/"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="legumemine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>

  <sources>

    <!-- chado genomics - bean, soybean -->
    <source name="chado-genomics1" type="legfed" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847 3885 3398"/>
      <property name="dataSetTitle" value="LIS chado genomic data"/>
      <property name="dataSourceName" value="LIS chado genomic data"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.SequenceProcessor"/>
    </source>

    <!-- chado genomics - peanut -->
    <source name="chado-genomics2" type="legfed" dump="true">
      <property name="source.db.name" value="peanutbase"/>
      <property name="organisms" value="3398 3817 3818 130453 130454"/>
      <property name="dataSetTitle" value="LIS Arachis data"/>
      <property name="dataSourceName" value="LIS Tripal2 database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.SequenceProcessor"/>
    </source>

    <!-- chado genetics - bean, soybean -->
    <source name="chado-genetics1" type="legfed" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847 3885 3398"/>
      <property name="dataSetTitle" value="LIS chado genetic data"/>
      <property name="dataSourceName" value="LIS chado genetic data"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GeneticProcessor"/>
    </source>

    <!-- chado genetics - peanut -->
    <source name="chado-genetics2" type="legfed" dump="true">
      <property name="source.db.name" value="peanutbase"/>
      <property name="organisms" value="3398 3817 3818 130453 130454"/>
      <property name="dataSetTitle" value="LIS Arachis data"/>
      <property name="dataSourceName" value="LIS Tripal2 database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GeneticProcessor"/>
    </source>

    <!-- chado featureprop - bean, soybean -->
    <source name="chado-featureprop1" type="legfed" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847 3885 3398"/>
      <property name="dataSetTitle" value="LIS Glycine max (3847) data"/>
      <property name="dataSourceName" value="LIS Tripal1 database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.FeaturePropProcessor"/>
    </source>

    <!-- chado featureprop - peanut -->
    <source name="chado-featureprop2" type="legfed" dump="true">
      <property name="source.db.name" value="peanutbase"/>
      <property name="organisms" value="3398 3817 3818 130453 130454"/>
      <property name="dataSetTitle" value="LIS Arachis data"/>
      <property name="dataSourceName" value="LIS Tripal2 database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.FeaturePropProcessor"/>
    </source>

    <!-- chado go - bean, soybean -->
    <source name="chado-go1" type="legfed" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847 3885 3398"/>
      <property name="dataSetTitle" value="LIS Phaseolus vulgaris (3885) data"/>
      <property name="dataSourceName" value="LIS Tripal1 database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GOProcessor"/>
    </source>

    <!-- chado go - peanut -->
    <source name="chado-go2" type="legfed" dump="true">
      <property name="source.db.name" value="peanutbase"/>
      <property name="organisms" value="3398 3817 3818 130453 130454"/>
      <property name="dataSetTitle" value="LIS Arachis data"/>
      <property name="dataSourceName" value="LIS Tripal2 database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GOProcessor"/>
    </source>

    <!-- soybean genetic markers from GFF file -->
    <source name="gff-geneticmarkers" type="legfed" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847"/>
      <property name="geneticMarkerGffFile" value="/home/intermine/data/soybean_map_version_4_0.gff3"/>
      <property name="dataSetTitle" value="Soybase genetic markers GFF dump"/>
      <property name="dataSourceName" value="LIS Tripal database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GeneticMarkerGFFProcessor"/>
    </source>

    <!-- soybean linkage group-QTL-marker relationships from tab-delimited CMap export file -->
    <source name="file-cmap" type="legfed" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847"/>
      <property name="cmapFile" value="/home/intermine/data/Soybean-GmComposite2003.txt"/>
      <property name="dataSetTitle" value="Soybase CMap linkage group-QTL-marker text file"/>
      <property name="dataSourceName" value="LIS Tripal database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.CMapFileProcessor"/>
    </source>

    <!-- soybean QTL-marker relationships from MySQL dump tab-delimited file -->
    <source name="file-qtlmarkers" type="legfed" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3847"/>
      <property name="qtlMarkerFile" value="/home/intermine/data/SoyBase-QTL-markers.txt"/>
      <property name="dataSetTitle" value="Soybase QTL-marker text file"/>
      <property name="dataSourceName" value="LIS Tripal database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.QTLMarkerFileProcessor"/>
    </source>

    <!-- bean to soybean synteny from GFF file -->
    <source name="file-synteny1" type="legfed" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="3885 3847"/>
      <property name="dagChainerFile" location="/home/intermine/data/synteny.3885_with_3847.gff3"/>
      <property name="dataSetTitle" value="LIS synteny dump"/>
      <property name="dataSourceName" value="LIS Tripal database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.SyntenyProcessor"/>
    </source>

    <!-- duranensis to ipaensis synteny from GFF file -->
    <source name="file-synteny2" type="legfed" dump="true">
      <property name="source.db.name" value="tripal"/>
      <property name="organisms" value="130453 130454"/>
      <property name="dagChainerFile" location="/home/intermine/data/synteny.130453_with_130454.gff3"/>
      <property name="dataSetTitle" value="Peanutbase synteny dump"/>
      <property name="dataSourceName" value="LIS Tripal database"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.SyntenyProcessor"/>
    </source>

    <!-- gene ontology -->
    <source name="go" type="go">
        <property name="src.data.file" location="/home/intermine/data/go-annotation/go.obo"/>
    </source>

    <!-- sequence ontology -->
    <source name="so" type="so">
      <property name="src.data.file" location="../../bio/sources/so/so.obo"/>
    </source>
      
    <!-- update organism records -->
    <source name="entrez-organism" type="entrez-organism">
      <property name="src.data.file" location="build/organisms.xml"/>
    </source>

  </sources>

  <post-processing>

    <!-- order from flymine -->
    <post-process name="populate-located-features"/>
    <post-process name="create-references" />
    <post-process name="create-chromosome-locations-and-lengths"/>
    <post-process name="create-intergenic-region-features"/>
    <post-process name="transfer-sequences"/>
    <post-process name="populate-child-features"/>
    <post-process name="do-sources" dump="true"/>
    <post-process name="create-location-range-index"/>
    <post-process name="create-location-overlap-index"/>
    <post-process name="create-overlap-view" />
    <post-process name="create-attribute-indexes"/>
    <post-process name="summarise-objectstore"/>
    <post-process name="create-autocomplete-index"/>
    <post-process name="create-search-index"/>

  </post-processing>

</project>
